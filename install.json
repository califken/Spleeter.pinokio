{
    "requires": [{ "type": "conda", "name": ["ffmpeg", "spleeter"], "args": "-c conda-forge" }],
    "run": [{
      "method": "shell.run",
      "params": {
        "message": "git clone https://github.com/deezer/spleeter"
      }
    }, {
      "method": "shell.run",
      "params": {
        "path": "spleeter",
        "venv": "venv",
        "message": [
          "pip install -r requirements.txt"
        ]
      }
    }, {
      "method": "input",
      "params": { "title": "Install Success", "description": "Spleeter is now installed. Go back to the dashboard to launch the app!" }
    }, {
      "method": "browser.open",
      "params": { "uri": "/?selected=Spleeter" }
    }]
  }
  